{
  "kind": "collectionType",
  "collectionName": "shopify_subscriptions",
  "info": {
    "singularName": "subscription",
    "pluralName": "subscriptions",
    "displayName": "Subscription",
    "name": "Subscription"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": [
        "PENDING",
        "ACTIVE",
        "DECLINED",
        "EXPIRED",
        "FROZEN",
        "CANCELLED"
      ],
      "required": true,
      "configurable": false
    },
    "trial_ends_on": {
      "type": "date",
      "configurable": false
    },
    "confirmation_url": {
      "type": "string",
      "configurable": false
    },
    "test": {
      "type": "boolean",
      "configurable": false
    },
    "shop": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::shopify.shop",
      "required": true,
      "configurable": false
    },
    "plan": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::shopify.plan",
      "required": true,
      "configurable": false
    },
    "usage_records": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::shopify.usage-record",
      "mappedBy": "subscription",
      "configurable": false
    },
    "shopify_id": {
      "type": "biginteger",
      "required": true,
      "unique": true,
      "configurable": false
    }
  }
}